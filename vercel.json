{
  "version": 2,
  "builds": [
    { "src": "Backend/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/Backend/server.js" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/styles.css", "dest": "/public/styles.css" },
    { "src": "/index.html", "dest": "/public/index.html" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
